# Email Intelligence Platform Environment Variables
# Copy this file to .env and fill in your values

# Secret key for the application (use a strong, random string)
SECRET_KEY=your-super-secret-key-here-replace-with-actual-key

# Debug mode (set to false in production)
DEBUG=false

# Data directory
DATA_DIR=./data

# AI Model API Keys (uncomment and set as needed)
# OPENROUTER_API_KEY=your-key-here
# GEMINI_API_KEY=your-key-here
# DEEPSEEK_API_KEY=your-key-here
# GROQ_API_KEY=your-key-here
# COHERE_API_KEY=your-key-here