  "installation_script": {
    "description": "Automated tool installation script",
    "commands": [
      "#!/bin/bash",
      "set -e",
      "",
      "detect_platform() {",
      "    case \"$(uname -s)\" in",
      "        Linux*)  PLATFORM=\"linux\" ;;",
      "        Darwin*) PLATFORM=\"macos\" ;;",
      "        CYGWIN*|MINGW*) PLATFORM=\"windows\" ;;",
      "        *) PLATFORM=\"unknown\" ;;",
      "    esac",
      "}",
      "",
      "install_tool() {",
      "    local tool_name=$1",
      "    echo \"Installing $tool_name...\"",
      "    ",
      "    # Get installation commands for current platform",
      "    local commands=$(jq -r \".tools.$tool_name.install_commands.$PLATFORM[]\" command_registry_tools.json 2>/dev/null)",
      "    ",
      "    if [ \"$commands\" = \"null\" ] || [ -z \"$commands\" ]; then",
      "        echo \"No installation commands found for $tool_name on $PLATFORM\"",
      "        return 1",
      "    fi",
      "    ",
      "    # Execute each command",
      "    echo \"$commands\" | while IFS= read -r cmd; do",
      "        if [ \"$cmd\" != \"null\" ] && [ -n \"$cmd\" ]; then",
      "            echo \"Running: $cmd\"",
      "            eval \"$cmd\" || {",
      "                echo \"Failed to execute: $cmd\"",
      "                return 1",
      "            }",
      "        fi",
      "    done",
      "    ",
      "    echo \"$tool_name installed successfully\"",
      "}",
      "",
      "main() {",
      "    detect_platform",
      "    echo \"Detected platform: $PLATFORM\"",
      "    ",
      "    for tool in \"$@\"; do",
      "        install_tool \"$tool\" || echo \"Failed to install $tool\"",
      "    done",
      "}",
      "",
      "main \"$@\""
    ]
  },
  "usage_examples": [
    "# Install single tool",
    "bash install_tools.sh qwen",
    "",
    "# Install multiple tools",
    "bash install_tools.sh qwen gemini aider",
    "",
    "# Install all tools",
    "bash install_tools.sh $(jq -r '.tools | keys[]' command_registry_tools.json)",
    "",
    "# Check available tools",
    "jq -r '.tools | keys[]' command_registry_tools.json"
  ]
