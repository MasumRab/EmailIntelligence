# Orchestration Tools Configuration

# GitHub
GITHUB_TOKEN=your_github_token_here
GITHUB_ORG=MasumRab
GITHUB_REPO=EmailIntelligence

# Git Configuration
GIT_USER_NAME=Your Name
GIT_USER_EMAIL=your.email@example.com

# Verification Configuration
VERIFICATION_PROFILE=default
LOG_LEVEL=INFO
LOG_FORMAT=json

# Environment
ENVIRONMENT=development
DEBUG=false

# Formal Verification
FORMAL_VERIFICATION_ENABLED=true
FORMAL_VERIFICATION_TIMEOUT=3600
FORMAL_VERIFICATION_COVERAGE_TARGET=0.99

# Token Optimization
TOKEN_OPTIMIZATION_ENABLED=true
TOKEN_LIMIT_PER_REQUEST=4000
TOKEN_LIMIT_PER_DAY=100000

# Performance
PARALLEL_EXECUTION=true
MAX_PARALLEL_TASKS=4
CACHE_ENABLED=true
CACHE_TTL=3600

# Security
ENABLE_ENCRYPTION=true
ENCRYPTION_KEY_PATH=.keys/encryption.key
AUTH_METHOD=token

# Logging
LOG_FILE_PATH=./logs/orchestration.log
STRUCTURED_LOGGING=true
CORRELATION_ID_PREFIX=ORCH
