{
  "id": "scientific",
  "name": "Scientific/AI Development Branch",
  "description": "Context profile for scientific branch with focus on AI analysis, email processing, and backend services",
  "branch_patterns": [
    "scientific",
    "scientific-*"
  ],
  "allowed_files": [
    "src/**",
    "tests/**",
    "docs/**",
    "*.md",
    "*.py",
    "*.sh",
    "*.yml",
    "*.yaml",
    "*.json",
    "*.ini",
    "*.cfg",
    "setup.py",
    "pyproject.toml",
    "requirements*.txt",
    "uv.lock",
    ".github/**",
    "CLAUDE.md",
    "README.md",
    "Dockerfile.backend",
    "docker-compose.yml"
  ],
  "blocked_files": [
    ".git/**",
    "__pycache__/**",
    "*.pyc",
    ".pytest_cache/**",
    "node_modules/**",
    "scripts/**",
    "specs/**",
    ".context-control/**",
    ".specify/**",
    ".taskmaster/**",
    "deployment/**",
    "setup/**"
  ],
  "agent_settings": {
    "enable_code_execution": true,
    "enable_file_writing": true,
    "enable_shell_commands": true,
    "max_context_length": 8192,
    "orchestration_aware": false
  },
  "project_config": {
    "project_name": "EmailIntelligenceAuto",
    "project_type": "backend-service",
    "max_context_length": 8192,
    "enable_code_execution": true,
    "enable_file_writing": true,
    "enable_shell_commands": true,
    "preferred_models": [
      "claude-3-5-sonnet",
      "gpt-4o",
      "claude-3-haiku"
    ]
  },
  "version": "1.0.0",
  "metadata": {
    "orchestration_disabled": true,
    "orchestration_disabled_timestamp": true
  }
}