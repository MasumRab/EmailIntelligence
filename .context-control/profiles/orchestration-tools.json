{
  "id": "orchestration-tools",
  "name": "Orchestration Tools Branch",
  "description": "Context profile for orchestration-tools branch with full access to orchestration scripts and tools",
  "branch_patterns": ["orchestration-tools", "orchestration-tools-*", "001-agent-context-control"],
  "allowed_files": [
    "scripts/**",
    "specs/**",
    "src/**",
    "tests/**",
    "docs/**",
    "*.md",
    "*.py",
    "*.sh",
    "*.yml",
    "*.yaml",
    "*.json",
    "*.ini",
    "*.cfg"
  ],
  "blocked_files": [
    ".git/**",
    "__pycache__/**",
    "*.pyc",
    ".pytest_cache/**",
    "node_modules/**"
  ],
  "agent_settings": {
    "enable_code_execution": true,
    "enable_file_writing": true,
    "enable_shell_commands": true,
    "max_context_length": 8192
  },
  "project_config": {
    "project_name": "EmailIntelligenceAuto",
    "project_type": "library",
    "max_context_length": 8192,
    "enable_code_execution": true,
    "enable_file_writing": true,
    "enable_shell_commands": true,
    "preferred_models": ["claude-3-5-sonnet", "gpt-4o", "claude-3-haiku"]
  },
  "version": "1.0.0"
}